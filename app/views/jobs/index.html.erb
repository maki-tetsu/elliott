<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Name</th>
    <th>Description</th>
    <th>Customer</th>
    <th>Due date</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Budget</th>
    <th>Estimate submitted date</th>
    <th>Register</th>
    <th>Register name</th>
    <th>Registed at</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.code %></td>
    <td><%= job.name %></td>
    <td><%= job.description %></td>
    <td><%= job.customer %></td>
    <td><%= job.due_date %></td>
    <td><%= job.start_date %></td>
    <td><%= job.end_date %></td>
    <td><%= job.budget %></td>
    <td><%= job.estimate_submitted_date %></td>
    <td><%= job.register_id %></td>
    <td><%= job.register_name %></td>
    <td><%= job.registed_at %></td>
    <td><%= job.note %></td>
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
